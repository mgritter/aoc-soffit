    "D1^D2^COMMA[R]",

    "INPUT[input]; LINE1[line]; LINE2[line]; END[end_of_input]; INPUT->LINE1->LINE2 [next_line]; LINE2->END [end_of_input]" :
    "LINE1[s_a]; LINE2[s_b]",

    "D[D]; D->START [end_of_line]; START[s_a]" :
